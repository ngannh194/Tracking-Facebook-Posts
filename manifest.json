{
  "name": "Facebook Audience",
  "version": "1.0",
  "description": "Fetch Facebook Audience in real time",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "identity",
    "identity.email",
    "notifications",
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}